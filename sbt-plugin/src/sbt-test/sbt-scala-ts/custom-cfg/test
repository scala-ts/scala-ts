> clean
$ absent target/_custom/scalatsName.ts
$ absent target/_custom/scalatsBar.ts
$ absent target/_custom/scalatsTransport.ts
$ absent target/_custom/scalatsBusLine.ts
$ absent target/_custom/scalatsTrainLine.ts
